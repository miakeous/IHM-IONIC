{
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "module": "commonjs",
    "target": "es5",
    "allowJs": true
  },
  "include": [
    "**/*.spec.ts",
    "src/**/*.spec.mock.ts",
    "src/**/*.spec.data.ts"
